generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_LOCAL")
}

model Past_Scores {
  Game_Num Int @id @default(autoincrement())
  Player_Name String?
  Score Int?
}

model User {
  Member_Number Int @id @default(autoincrement())
  Username String
  Password String
  PastGameScores String?
}